<%= title l(:label_good_new) %>

<%= labelled_form_for @good, :url => project_good_path(@project, @good),
                      :html => {:id => 'good-form', :multipart => true} do |f| %>
    <%= error_messages_for 'good' %>
    <div class="box tabular">
      <div id="all_attributes">
        <%= render :partial => 'form', :locals => {:f => f} %>
      </div>
    </div>

    <%= submit_tag l(:button_update) %>
<% end %>
